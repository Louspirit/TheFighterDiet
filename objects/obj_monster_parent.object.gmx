        // changer niveau perso
        lvl = 2;
    }
    else if(calorie &gt;= calorie_level_3 &amp;&amp; calorie &lt; calorie_level_4)
    {
        // lvl3
        // changer niveau perso
        lvl = 3;
    }
    else if(calorie &gt;= calorie_level_4)
    {
        lvl = 4;
    }
    
    // mettre à jour les stats (force etc..)
    objet_sur_la_table = table.id_obj_pickable.object_index;
    if(objet_sur_la_table == obj_gigot)
